<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="0dp"
        android:layout_height="150dp"
        android:background="@color/black"
        app:layout_constraintBottom_toTopOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="150dp"


    />

    <ImageView
        android:id="@+id/imageView_upper"
        android:layout_width="70dp"
        android:layout_height="70dp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintTop_toTopOf="@id/guideline"
        app:layout_constraintBottom_toBottomOf="@id/guideline"
        android:background="@drawable/profile"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Projectid- "
        android:textColor="@color/white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_margin="20dp"
        android:id="@+id/nameT"
        ></TextView>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/projectId"
        android:text="Abhinav"
        android:editable="false"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/nameT"
        android:textColor="@color/white"
        android:background="@android:color/transparent"
        app:layout_constraintBaseline_toBaselineOf="@id/nameT"
        android:layout_marginTop="20dp"

        ></EditText>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date- "
        android:textColor="@color/white"
        app:layout_constraintRight_toLeftOf="@id/date"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="20dp"
        android:id="@+id/Date"
        ></TextView>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/date"
        android:text="Abhinav"
        android:editable="false"
        app:layout_constraintBaseline_toBaselineOf="@id/Date"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textColor="@color/white"
        android:background="@android:color/transparent"
        android:layout_margin="20dp"

        ></EditText>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email- "
        android:textColor="@color/white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/nameT"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        android:id="@+id/mail"
        ></TextView>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Mail"
        android:text="kaka"
        android:editable="false"
        app:layout_constraintTop_toTopOf="@id/nameT"
        app:layout_constraintLeft_toRightOf="@id/mail"

        android:textColor="@color/white"
        android:background="@android:color/transparent"
        app:layout_constraintBaseline_toBaselineOf="@id/mail"
        android:layout_marginTop="20dp"

        ></EditText>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone- "
        android:textColor="@color/white"
        app:layout_constraintRight_toLeftOf="@id/Phone"
        app:layout_constraintTop_toBottomOf="@id/date"
        android:layout_marginTop="5dp"
        android:id="@+id/phone"
        ></TextView>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Phone"
        android:text="985554158"
        android:editable="false"
        app:layout_constraintBaseline_toBaselineOf="@id/phone"
        app:layout_constraintTop_toTopOf="@id/date"
        app:layout_constraintRight_toRightOf="parent"
        android:textColor="@color/white"
        android:background="@android:color/transparent"
        android:layout_margin="20dp"

        ></EditText>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Type- "
        android:textColor="@color/white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/phone"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:id="@+id/type"
        ></TextView>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Type"
        android:text="Abhinav"
        android:editable="false"
        app:layout_constraintBaseline_toBaselineOf="@id/type"
        app:layout_constraintTop_toTopOf="@id/phone"
        app:layout_constraintLeft_toRightOf="@id/type"
        android:textColor="@color/white"
        android:background="@android:color/transparent"


        ></EditText>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_edit_black_24dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="10dp"
        app:layout_constraintTop_toBottomOf="@id/imageView4"></ImageView>
<ListView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintTop_toBottomOf="@id/imageView4"
    app:layout_constraintLeft_toLeftOf="parent"
    android:layout_marginTop="180dp"
    android:id="@+id/list"
    ></ListView>




</androidx.constraintlayout.widget.ConstraintLayout>